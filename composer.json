{
    "name": "lembarek/auth",
    "description": "this allow to register and login using laravel",
    "keywords": [
        "lembarek",
        "auth"
    ],
    "homepage": "https://github.com/Lembarek/Auth",
    "license": "MIT",
    "authors": [
        {
            "name": "benghezala lembarek",
            "email": "lembarek2011@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.2",
        "php" : ">=5.3.0",
        "lembarek/core" : "dev-master@dev"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "codeception/codeception": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Lembarek\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable"
}
