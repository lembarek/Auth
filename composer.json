{
    "name": "Lembarek/Auth",
    "description": "this allow to register and login using laravel",
    "keywords": [
        "Lembarek",
        "Auth"
    ],
    "homepage": "https://github.com/Lembarek/Auth",
    "license": "MIT",
    "authors": [
        {
            "name": "benghezala lembarek",
            "email": "lembarek2011@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Lembarek\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lembarek\\Auth\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
